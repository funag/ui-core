{
  "extends": [
    "standard"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "max-lines": [
      "warn",
      {
        "max": 50,
        "skipComments": true
      }
    ],
    "react/jsx-uses-vars": "error"
  },
  "globals": {
    "APP_CONFIG": true
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
